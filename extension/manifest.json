{
    "manifest_version": 2,
    "name": "__MSG_ext_name__",
    "description": "This extension allows the user to claim what is deserved.",
    "version": "1.0",
    "browser_action": {
        "default_icon": {
            "16": "images/pwoff16.png",
            "24": "images/pwoff24.png",
            "32": "images/pwoff32.png",
            "64": "images/pwoff64.png",
            "128": "images/pwoff128.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "__MSG_ext_name__"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [],
            "js": [
                "contentscript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ]
}